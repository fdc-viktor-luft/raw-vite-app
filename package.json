{
  "name": "raw-vite-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "pnpm lint:cc && pnpm lint:es && pnpm lint:ts",
    "lint:fix": "pnpm lint:es -- --fix",
    "lint:es": "eslint --ext .ts,.tsx src --max-warnings 0",
    "lint:ts": "tsc",
    "lint:cc": "madge -c --extensions ts,tsx --ts-config tsconfig.json --no-spinner src/",
    "test": "jest --color --coverage"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.2.0",
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.1",
    "madge": "^5.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "spy4js": "^3.1.5",
    "typescript": "^4.4.3",
    "vite": "^2.5.10"
  }
}
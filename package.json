{
  "name": "raw-vite-app",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fdc-viktor-luft/raw-vite-app"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "pnpm lint:cc && pnpm lint:es && pnpm lint:ts",
    "lint:cc": "madge -c --extensions ts,tsx --ts-config tsconfig.json --no-spinner src/",
    "lint:es": "eslint . --max-warnings 0",
    "lint:fix": "pnpm lint:es --fix",
    "lint:ts": "tsc",
    "test": "vitest --run --coverage"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "9.2.0",
    "@testing-library/react": "15.0.7",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "9.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "jsdom": "24.0.0",
    "madge": "7.0.0",
    "prettier": "3.2.5",
    "sass": "1.77.1",
    "spy4js": "4.0.0-beta.1",
    "typescript": "5.4.5",
    "typescript-eslint": "7.9.0",
    "vite": "5.2.11",
    "vitest": "1.6.0"
  },
  "engines": {
    "node": ">= 20.13 < 21",
    "pnpm": ">= 9.1.1"
  }
}